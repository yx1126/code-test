{
    "manifest_version": 3,
    "name": "Input autofull",
    "description": "Input autofull",
    "version": "1.0.0",
    "icons": {
        "16": "imgs/icon_16.png",
        "32": "imgs/icon_32.png",
        "48": "imgs/icon_48.png",
        "128": "imgs/icon_128.png"
    },
    // "permissions": [
    //     // "webRequest",
    //     // "storage",
    //     // "declarativeNetRequest",
    // ],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
        {
            "js": [
                "inject.js",
                "render.js"
            ],
            "matches": ["<all_urls>"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["xhr.js"],
            "matches": ["<all_urls>"]
        }
    ]
}
